{
  "expo": {
    "name": "Great Learning Activities",
    "slug": "great-learning-activity-listing",
    "version": "1.0.0",
    "description": "A cross-platform activity listing application for online learning",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "greatlearningactivitylisting",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "primaryColor": "#3B82F6",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.greatlearning.activities"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.greatlearning.activities"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9521ebae-fd22-4280-91a2-3c6434527816"
      }
    }
  }
}
